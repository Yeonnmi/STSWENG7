<body class="regLogPage">
    <div id="registerContainer">
        <form action="/login" class="loginForm" method="Post" enctype="multipart/form-data">
            <p class="loginStyle" for="username">Username:</p>
            <input class="loginInput" type="text" id="logUsername" placeholder="Username" name="username">
            <p class="loginStyle" for="password">Password:</p>
            <input class="loginInput" type="password" id="password" placeholder="Password" name="password">
            <button class="loginSubmit" type="submit" id="login">LOGIN</button>
        </form>
    </div>
</body>
