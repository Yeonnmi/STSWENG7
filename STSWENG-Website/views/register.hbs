<body class="regLogPage">
    <div id="registerContainer">
        <form action="/register" class="registerForm" method="Post" enctype="multipart/form-data">
            <p class="registerStyle" for="username">Username:</p>
            <input class="registerInput" type="text" id="regUsername" placeholder="Username" name="username">
            <p class="registerStyle" for="password">Password:</p>
            <input class="registerInput" type="password" id="password" placeholder="Password" name="password">
            <p class="registerStyle" for="password">Retype Password:</p>
            <input class="registerInput" type="password" id="Repassword" placeholder="Retype Password" name="Repassword">
            <button class="registerSubmit" type="submit" id="login">REGISTER</button>
        </form>
    </div>
</body>
